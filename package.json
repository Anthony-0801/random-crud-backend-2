{
  "name": "random-crud-backend-2",
  "version": "1.0.0",
  "description": "A simple backend for a random CRUD application using Node.js and Express",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "start": "dotenv -- node dist/index.js",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "prisma:generate": "prisma generate",
    "migration:create": "prisma migrate dev --create-only",
    "migrate": "prisma migrate deploy",
    "db:seed": "prisma db seed",
    "dev": "dotenv -- tsc-watch --onSuccess \"node dist/index.js\""
  },
  "keywords": [],
  "author": "Anthony A. Cabulang",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@faker-js/faker": "^9.8.0",
    "@tsconfig/node22": "^22.0.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/module-alias": "^2.0.4",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.0.3",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-jest": "^28.14.0",
    "eslint-plugin-prettier": "^5.4.1",
    "globals": "^16.2.0",
    "jest": "^30.0.0",
    "prettier": "^3.5.3",
    "prisma": "^6.10.1",
    "ts-jest": "^29.4.0",
    "tsc-watch": "^7.1.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.0"
  },
  "dependencies": {
    "@prisma/client": "^6.10.1",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "express-oauth2-jwt-bearer": "^1.6.1",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "prisma": {
    "seed": "npx ts-node prisma/seed.ts"
  },
  "_moduleAliases": {
    "@": "dist",
    "@src": "src",
    "@prisma": "prisma",
    "@config": "src/config",
    "@controllers": "src/controllers",
    "@middlewares": "src/middlewares",
    "@routes": "src/routes",
    "@errors": "src/errors",
    "@tests": "tests",
    "@data": "src/data"
  }
}
